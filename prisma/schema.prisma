generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma/client"
  engineType      = "client"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        Int      @id @default(autoincrement())
  firstName String
  lastName  String
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}